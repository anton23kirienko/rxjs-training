{
  "compilerOptions": {
    "module": "esnext",
    "esModuleInterop": true,
    "target": "esnext",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "isolatedModules": false,
    "strict": false,
    "noImplicitAny": false
  },
  "ts-node": {
    "esm": true
  },
  "exclude": ["node_modules"],
  "lib": ["esnext"]
}
